{
  "name": "game",
  "version": "0.0.1",
  "description": "Учебный проект команды Minneapolis-7 на Yandex Praktikum",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --watch ./webpack/webpack.config.js --watch ./webpack/webpack.development.js --exec \"webpack serve --config ./webpack/webpack.config.js --env mode=development\"",
    "clean": "rimraf ./dist",
    "build:front": "npm run clean && webpack --config ./webpack/webpack.config.js --env mode=production",
    "build:server": "webpack --config ./webpack/webpack.server.js --env mode=production",
    "build": "run-s build:front build:server",
    "build:analyze": "npm run build:front -- --env presets=analyze",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js --config ./webpack/webpack.config.js",
    "debug:dev": "npm run debug -- --env mode=development",
    "debug:prod": "npm run debug -- --env mode=production",
    "lint:css": "stylelint **/*.scss --fix",
    "lint:js": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "lint": "concurrently npm:lint:*",
    "pre-commit": "npm run lint",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Minneapolis-7/game.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Minneapolis-7/game/issues"
  },
  "homepage": "https://github.com/Minneapolis-7/game#readme",
  "browserslist": [
    "last 2 versions",
    "not IE 11"
  ],
  "dependencies": {
    "axios": "0.21.1",
    "bem-cn": "3.0.1",
    "bootstrap-icons": "1.5.0",
    "date-fns": "2.23.0",
    "express": "4.17.1",
    "html5-boilerplate": "8.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0"
  },
