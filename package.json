{
  "name": "game",
  "version": "0.0.1",
  "description": "Учебный проект команды Minneapolis-7 на Yandex Praktikum",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development npm-run-all --print-label --parallel start:*",
    "start:webpack": "nodemon --watch webpack --exec \"webpack watch --env mode=development --config ./webpack/webpack.config.js\"",
    "start:server": "nodemon -r dotenv/config start-server.js --watch ./dist/server.js",
    "clean": "rimraf ./dist",
    "build": "NODE_ENV=production npm run clean && webpack --env mode=production --config ./webpack/webpack.config.js",
    "build:analyze": "npm run build -- --env presets=analyze",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js --config ./webpack/webpack.config.js",
    "debug:dev": "npm run debug -- --env mode=development",
    "debug:prod": "npm run debug -- --env mode=production",
    "lint:css": "stylelint **/*.scss --fix",
    "lint:js": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "lint": "concurrently npm:lint:*",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Minneapolis-7/game.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Minneapolis-7/game/issues"
  },
